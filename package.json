{
  "name": "async2sync2",
  "version": "0.0.2",
  "main": "index.js",
  "description": "A tool that converts asynchronous functions into synchronous functions. It achieves synchronous execution of asynchronous functions by extracting the function source code into a separate JavaScript file and running it.",
  "scripts": {
    "build": "npx ncc build node_modules/serialize-javascript/index.js -o serialize-javascript",
    "test": "vitest --reporter=basic"
  },
  "homepage": "https://github.com/wll8/async2sync2",
  "repository": {
    "type": "git",
    "url": "git://github.com/wll8/async2sync2.git"
  },
  "keywords": ["async", "sync"],
  "author": "wll8",
  "license": "MIT",
  "files": [
    "index.js",
    "serialize-javascript"
  ],
  "dependencies": {
    "serialize-javascript": "^6.0.2"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.3",
    "vitest": "^2.1.8"
  }
}
